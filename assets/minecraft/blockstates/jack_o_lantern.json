{
	"multipart": [
		{
			"when": {"facing": "north"},
			"apply": [
				{"model": "block/jack_o_lantern"},
				{"model": "block/jack_o_lantern_0"},
				{"model": "block/jack_o_lantern_1"},
				{"model": "block/jack_o_lantern_2"},
				{"model": "block/jack_o_lantern_3"}
			]
		},
		{
			"when": {"facing": "east"},
			"apply": [
				{"model": "block/jack_o_lantern", "y": 90},
				{"model": "block/jack_o_lantern_0", "y": 90},
				{"model": "block/jack_o_lantern_1", "y": 90},
				{"model": "block/jack_o_lantern_2", "y": 90},
				{"model": "block/jack_o_lantern_3", "y": 90}
			]
		},
		{
			"when": {"facing": "south"},
			"apply": [
				{"model": "block/jack_o_lantern", "y": 180},
				{"model": "block/jack_o_lantern_0", "y": 180},
				{"model": "block/jack_o_lantern_1", "y": 180},
				{"model": "block/jack_o_lantern_2", "y": 180},
				{"model": "block/jack_o_lantern_3", "y": 180}
			]
		},
		{
			"when": {"facing": "west"},
			"apply": [
				{"model": "block/jack_o_lantern", "y": 270},
				{"model": "block/jack_o_lantern_0", "y": 270},
				{"model": "block/jack_o_lantern_1", "y": 270},
				{"model": "block/jack_o_lantern_2", "y": 270},
				{"model": "block/jack_o_lantern_3", "y": 270}
			]
		},
		{
			"when": {"facing": "north"},
			"apply": {"model": "block/pumpkin_candle"}
		},
		{
			"when": {"facing": "east"},
			"apply": {"model": "block/pumpkin_candle", "y": 90}
		},
		{
			"when": {"facing": "south"},
			"apply": {"model": "block/pumpkin_candle", "y": 180}
		},
		{
			"when": {"facing": "west"},
			"apply": {"model": "block/pumpkin_candle", "y": 270}
		}
	]
}
