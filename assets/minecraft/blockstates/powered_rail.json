{
	"comment": "Designed by Juknum",
	"multipart": [
		{
			"when": { "shape": "north_south" },
			"apply": { "model": "minecraft:3d/rail/flat" }
		},
		{
			"when": { "shape": "east_west" },
			"apply": { "model": "minecraft:3d/rail/flat", "y": 90 }
		},

		{
			"when": { "powered": true, "shape": "north_south" },
			"apply": { "model": "minecraft:3d/rail/powered_on" }
		},
		{
			"when": { "powered": false, "shape": "north_south" },
			"apply": { "model": "minecraft:3d/rail/powered" }
		},
		{
			"when": { "powered": true, "shape": "east_west" },
			"apply": { "model": "minecraft:3d/rail/powered_on", "y": 90 }
		},
		{
			"when": { "powered": false, "shape": "east_west" },
			"apply": { "model": "minecraft:3d/rail/powered", "y": 90 }
		},
		{
			"when": { "shape": "ascending_north" },
			"apply": { "model": "minecraft:3d/rail/raised" }
		},
		{
			"when": { "shape": "ascending_south" },
			"apply": { "model": "minecraft:3d/rail/raised", "y": 180 }
		},
		{
			"when": { "shape": "ascending_east" },
			"apply": { "model": "minecraft:3d/rail/raised", "y": 90 }
		},
		{
			"when": { "shape": "ascending_west" },
			"apply": { "model": "minecraft:3d/rail/raised", "y": -90 }
		},

		{
			"when": { "shape": "ascending_north", "powered": true },
			"apply": { "model": "minecraft:3d/rail/powered_raised_on" }
		},
		{
			"when": { "shape": "ascending_south", "powered": true },
			"apply": { "model": "minecraft:3d/rail/powered_raised_on", "y": 180 }
		},
		{
			"when": { "shape": "ascending_east", "powered": true },
			"apply": { "model": "minecraft:3d/rail/powered_raised_on", "y": 90 }
		},
		{
			"when": { "shape": "ascending_west", "powered": true },
			"apply": { "model": "minecraft:3d/rail/powered_raised_on", "y": -90 }
		},
		{
			"when": { "shape": "ascending_north", "powered": false },
			"apply": { "model": "minecraft:3d/rail/powered_raised" }
		},
		{
			"when": { "shape": "ascending_south", "powered": false },
			"apply": { "model": "minecraft:3d/rail/powered_raised", "y": 180 }
		},
		{
			"when": { "shape": "ascending_east", "powered": false },
			"apply": { "model": "minecraft:3d/rail/powered_raised", "y": 90 }
		},
		{
			"when": { "shape": "ascending_west", "powered": false },
			"apply": { "model": "minecraft:3d/rail/powered_raised", "y": -90 }
		}
	]
}
