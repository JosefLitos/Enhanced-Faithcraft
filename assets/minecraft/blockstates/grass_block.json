		},
		{
			"when": {
				"snowy": false
			},
			"apply": [
				{ "model": "block/empty", "weight": 200 },
				{ "model": "3d/grass_block/0" },
				{ "model": "3d/grass_block/1" },
				{ "model": "3d/grass_block/2" },
				{ "model": "3d/grass_block/3" }
			]
		},
		{
			"when": { "snowy": true },
			"apply": [
				{ "model": "block/grass_block_snow" },
				{ "model": "block/dirty_side_blocks/grass_snow_0" },
				{ "model": "block/dirty_side_blocks/grass_snow_1" },
				{ "model": "block/dirty_side_blocks/grass_snow_2" },
				{ "model": "block/dirty_side_blocks/grass_snow_3" },
				{ "model": "block/dirty_side_blocks/grass_snow_4" }
			]
		}
