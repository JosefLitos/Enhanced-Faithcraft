				{ "model": "block/dirty_side_blocks/grass_overlay_4", "y": 270 },
				{ "model": "block/dirty_side_blocks/grass_overlay_5" },
				{ "model": "block/dirty_side_blocks/grass_overlay_5", "y": 90 },
