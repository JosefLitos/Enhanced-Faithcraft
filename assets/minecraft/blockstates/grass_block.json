{
	"multipart": [
		{
			"when": { "snowy": false },
			"apply": [
				{ "model": "block/grass_block" },
				{ "model": "block/grass_block", "y": 90 },
				{ "model": "block/grass_block", "y": 180 },
				{ "model": "block/grass_block", "y": 270 },

				{ "model": "block/dirty_side_blocks/grass_overlay_0" },
				{ "model": "block/dirty_side_blocks/grass_overlay_0", "y": 90 },
				{ "model": "block/dirty_side_blocks/grass_overlay_0", "y": 180 },
				{ "model": "block/dirty_side_blocks/grass_overlay_0", "y": 270 },
				{ "model": "block/dirty_side_blocks/grass_overlay_1" },
				{ "model": "block/dirty_side_blocks/grass_overlay_1", "y": 90 },
				{ "model": "block/dirty_side_blocks/grass_overlay_1", "y": 180 },
				{ "model": "block/dirty_side_blocks/grass_overlay_1", "y": 270 },
				{ "model": "block/dirty_side_blocks/grass_overlay_2" },
				{ "model": "block/dirty_side_blocks/grass_overlay_2", "y": 90 },
				{ "model": "block/dirty_side_blocks/grass_overlay_2", "y": 180 },
				{ "model": "block/dirty_side_blocks/grass_overlay_2", "y": 270 },
				{ "model": "block/dirty_side_blocks/grass_overlay_2" },
				{ "model": "block/dirty_side_blocks/grass_overlay_3", "y": 90 },
